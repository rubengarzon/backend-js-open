    "typescript",
    "swagger",
    "mongoose"
  ],
  "author": "@rubengarzon",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rubengarzon/backend-js/issues"
  },
  "homepage": "https://github.com/rubengarzon/backend-js#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "helmet": "^6.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.0.1",
    "@types/node": "^18.7.16",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "concurrently": "^7.4.0",
    "eslint": "^8.23.1",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-promise": "^6.0.1",
    "jest": "^29.0.3",
    "nodemon": "^2.0.19",
    "serve": "^14.0.1",
    "supertest": "^6.2.4",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  }
